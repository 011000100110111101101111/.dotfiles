{
  "Parameters": {
    "cmd_focused_window": "hyprctl activewindow",
    "importer": {
      "match_line": ".*::(.+?)::(.+?)::(.+?)::.*",
      "position_group": 1,
      "position_keybinding": 2,
      "position_label": 3
    },
    "keys": {
      "tab_previous": "Backtab",
      "tab_next": "Tab",
      "scroll_down": "Down",
      "scroll_up": "Up",
      "quit": "Esc"
    },
    "layout": {
      "width": 1500,
      "height": 1200,
      "n_columns": 3,
      "spacing": 3,
      "background_color": "#282C34",
      "opacity": 0.95,
      "tabs": {
        "background_color": "#282C34",
        "background_color_current": "#3E4451",
        "color": "#E5E9F0",
        "font": "Dancing Script",
        "font_size": 12,
        "padding": 0
      },
      "group_name": {
        "background_color": "#FFF08F",
        "color": "#000000",
        "bold": true,
        "stretched": false,
        "padding": "0px 5px 0px 2px",
        "font": "Lato",
        "font_size": 10
      },
      "shortcut": {
        "label": {
          "color": "#EEEEEE",
          "bold": false,
          "font": "Lato",
          "font_size": 10
        },
        "value": {
          "color": "#D19A66",
          "bold": true,
          "font": "Lato",
          "font_size": 10
        },
        "spacing": 5
      }
    }
  },
